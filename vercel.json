{
  "version": 2,
  "builds": [{ "src": "/app.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/data", "dest": "/app.js" },
    { "src": "/", "dest": "/app.js" },
    { "src": "/getme", "dest": "/app.js" }
  ],
  "functions": {
    "api/*": {
      "timeout": 30 // Increase timeout to 30 seconds (or adjust as needed)
    }
  }
}
